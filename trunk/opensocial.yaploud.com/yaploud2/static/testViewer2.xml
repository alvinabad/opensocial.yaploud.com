
<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="Test Viewer2" 
               author="Alvin Abad" 
               author_email="alvin.abad@yaploud.com" 
               description="Yaploud OpenSocial Chat Application">
    <Require feature="opensocial-0.8" />
    <Require feature="dynamic-height" />
  </ModulePrefs>
    
  <Content type="html" >
  <![CDATA[
<script type="text/javascript" 
     src="http://jqueryjs.googlecode.com/files/jquery-1.3.2.min.js"></script>
     
<script type="text/javascript">
    function init() {
        jQuery("#view_div").css("width", "500px");
        jQuery("#view_div").css("height", "600px");
        jQuery("#view_div").css("border", "solid red 2px");
    }
    gadgets.util.registerOnLoadHandler(init);
    //gadgets.window.adjustHeight();
</script>

    <!-- The DIV view is here. -->
    <div id="view_div"></div>
]]>
</Content>
</Module>